pipeline{
    agent any
    
    stages{
          stage('git checkout
                steps{
                        git 'https://github.com/AbdulAhadl/AssignmentProject.git'
          }
          }
  
          stage('mvn build'){
                steps{
                        sh 'tool name: 'mvn-build', type: 'maven''
                        sh 'mvn clean package'
                
                }
          }
    
    }

}
