pipeline{
    agent any
    environment{
        PATH = "opt/apache-maven-3.9.0/bin:$PATH"
        }
    stages{
          stage("git checkout"){
                steps{
                       git credentialsId: 'vickyfit2022', url: 'https://github.com/AbdulAhadl/AssignmentProject.git'
          }
          }
  
          stage("Maven build"){
                steps{
                    sh "mvn clean package"
               }
          }
    
    }

}
