pipeline{
   agent any
   environment{
        path = "/opt/apache-maven-3.8.7/bin:$PATH"
   }
    stages{
        stage('git checkout'){
            steps{
                git credentialsId: 'vickyfit2022', url: 'https://github.com/AbdulAhadl/AssignmentProject.git'
            }
        }
        stage('mvn build'){
            steps{
               sh "mvn clean package"
             }
        }
    }
    
}
